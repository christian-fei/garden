SHELL=/bin/bash
*/30 * * * * sh /home/pi/garden/current/take-current-snapshot >> /home/pi/snapshots.log 2>&1
* * * * * sh /home/pi/garden/current/take-last-snapshot >> /home/pi/snapshots.log 2>&1
10 * * * * /home/pi/.local/bin/aws s3 sync /home/pi/snapshots/ s3://garden-snapshots >> /home/pi/s3-backup.log 2>&1
10 */6 * * * sh /home/pi/garden/current/clean-snapshots >> /home/pi/clean-snapshots.log 2>&1